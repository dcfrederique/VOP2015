@{
    ViewBag.Title = "Rooms";
}
<h2 id="pageHeader"></h2>
<div class="row">
    <div id="rooms" class="col-md-6">

    </div>
</div>
@section Scripts{
    <script>
        var startRoomChat, stopRoomChat;
        var globalUserObj = {
            userId: "@(ViewBag.UserID)",
            userName: "@(ViewBag.UserName)",
            hasJoined: "@(ViewBag.Joined)" === "True" ? true : false,
            roomId: "@(ViewBag.RoomId)",
            hasStarted: false
        };
    </script>
    @Scripts.Render("~/bundles/templater")
    @Scripts.Render("~/Scripts/jquery.signalR-2.1.2.min.js")
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="@Url.Content("~/signalr/hubs")"></script>
    @Scripts.Render("~/Scripts/custom/roomchat.js")
    @Scripts.Render("~/Scripts/custom/rooms.js")
}